{{#if messages}}
    <div class="alert alert-success">{{messages}}</div>
{{/if}}
<h1><i class="fa fa-safari" aria-hidden="true"></i> My History</h1>
<hr>
<table id="example" class="table table-striped table-bordered dt-responsive nowrap" cellspacing="0" width="100%">
        <thead>
            <tr>
                <th>Tittle of Journey</th>
                <th>Descriptions</th>
                <th>Status</th>
                <th>Action</th>
            </tr>
        </thead>
        <tfoot>
            <tr>
                <th>Tittle of Journey</th>
                <th>Descriptions</th>
                <th>Status</th>
                <th>Action</th>
            </tr>
        </tfoot>
        <tbody>
            {{#each data}}
            <tr>
                <td>{{this.tittle}}</td>
                <td>{{this.descriptions}}</td>
                <td>{{this.status}}</td>
                <td>
                    <a href="/user/history-details/{{this._id}}" class="btn btn-primary btn-circle"><i class="fa fa-info-circle" aria-hidden="true"></i></a>
                    <a href="/user/history-edit/{{this._id}}" class="btn btn-warning btn-circle"><i class="fa fa-pencil-square" aria-hidden="true"></i></a>
                    <a class="btn btn-danger btn-circle" onclick="confirmDelete('/user/history-delete/{{this._id}}')"><i class="fa fa-trash-o" aria-hidden="true"></i></a>
                </td>
            </tr>
            {{/each}}
        </tbody>
    </table>
<hr>
<a href="/user/create-journey" class="btn btn-primary">Add New</a>